meta {
  name: Start Evaluation (All Data)
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/evaluation
  body: json
  auth: none
}

body:json {
  {
    "last_x_days": -1,
    "re_calculate": true,
    "evaluation_run": false
  }
}

tests {
  test("Status is 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response contains job info", function() {
    const body = res.getBody();
    expect(body.job_id).to.not.be.undefined;
    expect(body.start_time).to.not.be.undefined;
    expect(['started', 'completed']).to.include(body.status);
    expect(body.progress).to.not.be.undefined;
  });
}
